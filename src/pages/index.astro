---
// src/pages/index.astro
import BaseLayout from '../layouts/DefautLayout.astro';
import VideoHeader from '../components/VideoHeader.svelte';
import ClubIntro from '../components/ClubIntro.svelte';
import TeamsMens from '../components/TeamsMens.svelte';
import TeamsWomens from '../components/TeamsWomens.svelte';
import Socials from '../components/Socials.svelte';
import Instagram from '../components/Instagram.svelte';
import Contact from '../components/Contact.svelte';
import Footer from '../components/Footer.svelte';

let baseUrl = process.env.VERCEL_ENV_URL
---


<BaseLayout title="Home">
    <VideoHeader />
    <ClubIntro />
    <TeamsMens />
    <TeamsWomens />
    <Socials />
    <Instagram baseUrl={baseUrl} client:idle />
    <Contact client:idle />

    <Footer />
    

</BaseLayout>